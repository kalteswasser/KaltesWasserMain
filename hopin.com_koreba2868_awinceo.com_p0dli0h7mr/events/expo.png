      for (var i = 0; i < select.length; i++) {
        data[select[i].name] = select[i].options[select[i].selectedIndex].value;
      }

      console.log(data);
      document.querySelector('.modal-form').style.display = "none";
      dataLayer.push({
        "ModalForm":"Submitted",
        "event":"Footer Modal Form Submit"
      });
