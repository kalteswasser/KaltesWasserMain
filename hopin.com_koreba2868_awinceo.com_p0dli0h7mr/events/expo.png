  	});
    
    $(".btn-close, .modal-backdrop").click(function(){
    	$("body").removeClass("modal-open");
      	$(this).closest('.modal').fadeOut();
  	});
    
    if($(window).width() > 767) {
        $("#sticky_item").stick_in_parent({
            offset_top: $('.header').innerHeight()
        });
    }
    
    $('.card_header_btn').on('click', function(e) {
    	e.preventDefault();
    	var thP = $(this).closest('.accordion_block');
      	if($(this).hasClass('card_header_btn_modal') && !thP.hasClass('open')) {
        	$(this).closest('.accordion').find('.collapse').slideUp();
        	$(this).closest('.accordion').find('.accordion_block').removeClass('open');
    		setTimeout(function () {
          		$(".modal-scroll-block").getNiceScroll().resize();
    		}, 500);
        } else if($(this).hasClass('card_header_btn_modal')) {
        	thP.removeClass('open');
        	thP.find('.collapse').slideUp();
    		setTimeout(function () {
          		$(".modal-scroll-block").getNiceScroll().resize();
    		}, 500);
        	return false;
        }
    	thP.toggleClass('open');
    	thP.find('.collapse').slideToggle();
    });
    
    $('.accordion_block').each(function(e) {
      if(!$(this).hasClass('open')) {
        $(this).find('.collapse').hide();
      }
    });
    
    
    $(window).on('resize orientationchange', function () {
        $(".modal-scroll-block").getNiceScroll().resize();
    });

    $('.modal-scroll-block').niceScroll({
        cursorborder: "3",
        cursorcolor:'#BDBFC7',
        cursorwidth: 3,
        cursorborderradius: 0,
        horizrailenabled: false,
        background: "#ffffff",
    });
    
    
    $('.mobile-plan-detail .btn_show').on('click', function (e) {
        e.preventDefault();
        $(this).closest('.mobile-plan-detail').addClass('active');
        $('.price-block-body').slideDown();
    });

    $('.mobile-plan-detail .btn_hide').on('click', function (e) {
        e.preventDefault();
        $(this).closest('.mobile-plan-detail').removeClass('active');
        $('.price-block-body').slideUp();
    });

    $('.hide-careers-intro-mess').on('click', function (e) {
        e.preventDefault();
