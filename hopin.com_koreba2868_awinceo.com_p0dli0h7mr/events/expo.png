      $.each(data.marketplace_category.events, function(e){
         thHtml = $('.events_template');
         $(thHtml).find('.events_item').attr('href', $(this)[0].url);
         $(thHtml).find('.events_img').attr('src', $(this)[0].images.large);
         $(thHtml).find('.event_title').text($(this)[0].organization.name);
         $(thHtml).find('.event_date').text($(this)[0].start_time);
         if($(this)[0].min_price == $(this)[0].max_price && $(this)[0].max_price == 0){
         		$(thHtml).find('.event_data').text('Free');
         } else if($(this)[0].min_price == $(this)[0].max_price) {
         		$(thHtml).find('.event_data').text($(this)[0].currency_symbol+$(this)[0].max_price+'.00');
         } else {
         		$(thHtml).find('.event_data').text($(this)[0].currency_symbol+$(this)[0].min_price+'.00 ~ '+$(this)[0].currency_symbol+$(this)[0].max_price+'.00');
         }
         $('.events_slider').append($(thHtml).html());
      })
      $('.events_slider .event_date').each(function() {
            const utcDateTimeString = $(this).html()
            const date = new Date(utcDateTimeString);
            const localDateTimeString = new Intl.DateTimeFormat([], {
              month: 'short',
              day: 'numeric',
              hour: 'numeric',
              minute: 'numeric',
              timeZoneName: 'short'
            }).format(date);
            $(this).html(localDateTimeString);
      });  
		});
    
</script>
